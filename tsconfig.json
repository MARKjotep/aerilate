{
  "compilerOptions": {
    "noImplicitAny": true,
    // "checkJs": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "lib": ["ESNext", "DOM"],
    "noImplicitThis": false,
    "outDir": "../../dist/aerilate",
    "composite": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "baseUrl": "./"
  }
}
